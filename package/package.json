{
  "name": "mantine-gantt",
  "version": "1.0.0",
  "description": "A gantt chart component for mantine",
  "homepage": "https://github.com/WojakGra/mantine-gantt#readme",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Jakub Wojdziak <jakub@wojak.works>",
  "keywords": [
    "extension",
    "gantt",
    "mantine"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "repository": "https://github.com/WojakGra/mantine-gantt.git",
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/utilities": "^3.2.2",
    "dayjs": "^1.11.10"
  },
  "bugs": "https://github.com/WojakGra/mantine-gantt/issues"
}
